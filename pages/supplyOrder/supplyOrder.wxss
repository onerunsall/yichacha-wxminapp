/* pages/supplyOrder/supplyOrder.wxss */

page {
  background: #f2f2f2;
}

.orderItem {
  width: 642rpx;
  height: 376rpx;
  border-radius: 10rpx;
  margin: 24rpx 24rpx 0;
  background: #fff;
  padding: 30rpx 30rpx 0 30rpx;
}

.orderItem>text {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 34rpx;
  line-height: 42rpx;
   margin-bottom: 25rpx;
   height: 84rpx;
  /* margin: 30rpx 0; */
}
.orderItem>view{
 
  font-size: 26rpx;
  color: rgb(51,51,51);
  margin-bottom: 20rpx;
}
.orderItem>view>text{
  color: rgb(153,153,153);
}

.orderPrice{
  margin-top: 26rpx;
  height: 98rpx;
  width: 638rpx;
  /* margin-left: 32rpx; */
  border-top: 1rpx solid #e5e5e5;
}
.orderPrice text:nth-child(1){
  float: right;
  color: rgb(255,149,27);
  line-height: 98rpx;
}
.orderPrice text:nth-child(2){
  float: right;
  color: rgb(153,153,153);
  margin-right: 20rpx;
  line-height: 98rpx;
}