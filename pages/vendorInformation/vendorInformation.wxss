/* pages/vendorInformation/vendorInformation.wxss */
page{
  width: 750rpx;
  overflow-x: hidden;
}
.DExhibitionHall{
  width:686rpx;
  height: 360rpx;
  margin: 0 auto;
  border-radius: 10rpx; 
}
.DExhibitionHall image{
  width: 98rpx;
  height: 98rpx;
  margin:131rpx  294rpx ;
}
.DExhibitionHall text{
  float: right;
  display: inline-block;
  width: 120rpx;
  height: 30rpx;
  background: rgba(0,0,0,0.5);
  border:1px solid  rgba(72,72,72,0.8);
  font-size: 22rpx;
  line-height: 30rpx;
  text-align: center;
  margin-top: -55rpx;
  margin-right: 20rpx;
  color: #fff;
}

.manufacturer>text:nth-child(1){
  display: block;
  font-size: 40rpx;
  color: rgb(51,51,51);
  line-height: 40rpx;
  /* height: 40rpx; */
  margin: 44rpx 32rpx 24rpx;
}
.manufacturer>view:nth-child(2) {
  width: 686rpx;
  margin:  0 32rpx;
  border-bottom: 1rpx solid rgb(238,238,238);
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 0 24rpx 0;
}
.manufacturer>view:nth-child(2) image{
  width: 34rpx;
  height: 30rpx;
  float: left;
  margin-right: 8rpx;
}

.manufacturer>view:nth-child(2) text{
  font-size: 24rpx;
  line-height: 30rpx;
  color: rgb(251,119,0);
  float: left;
}
.addPhone{
  /* height: 32rpx; */
