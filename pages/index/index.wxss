}
.csTitle{
  margin-top: 36rpx;
}
.csTitle>view:first-child{
  
  line-height: 30rpx;
  height: 30rpx;
  overflow-y: hidden;
}
.csTitle>view:first-child>text:nth-child(1){
  max-width: 530rpx;
  
display:-webkit-box;
-webkit-line-clamp:1;
-webkit-box-orient:vertical;
overflow:hidden;
}
