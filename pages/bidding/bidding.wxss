/* pages/bidding/bidding.wxss */
page{
  background: #f2f2f2;
}
.bidItem{
  width: 100%;
  height: 308rpx;
  background: #fff;
  margin-top: 24rpx;
}
.bidItemLf{
  width: 706rpx;
  float: left;
}
.bidItemLf>text{
  margin-left: 24rpx;
  font-size: 36rpx;
  line-height: 55rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color: rgb(51,51,51);
  font-family: PingFangSC-Mediun;
  font-weight: 400;
   display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.bidItemLf>view{
  margin-left: 24rpx;
  font-size: 26rpx;
  color: rgb(51,51,51);
  line-height: 26rpx;
  margin-bottom: 28rpx;
    overflow:hidden;
white-space:nowrap;
text-overflow:ellipsis;
}
.bidItemLf>view>text{
  color: rgb(153,153,153);
}
.bidItemRt{
  width: 44rpx;
  float: left;
}
.bidItemRt>image{
  width: 20rpx;
  height: 32rpx;
  margin-top: 138rpx;
}