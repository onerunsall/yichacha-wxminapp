/* pages/mycollection/mycollection.wxss */
page{
  width: 100%;
  height: 100%;
  background: #fff !important;
}
.withoutNone{
  width: 100%;
  height: 100%;
  background: #fff;
}
.withoutNone image{
  width: 286rpx;
  height: 220rpx;
  margin: 260rpx 232rpx 56rpx 232rpx;
}
.withoutNone text{
  font-size: 30rpx;
  color: rgb(153,153,153);
  width: 100%;
  text-align: center;
  display: block;
}

.instrumentList{
  width: 750rpx;
  /* height: 500rpx; */
  height: auto;
  margin: 0 auto;
  padding: 30rpx 12rpx;
  background: #fff;
}
.instrument{
  float: left;
    box-shadow:-2rpx 8rpx 10px rgba(0,0,0,0.1);
   /* box-shadow: 2rpx 2rpx 4rpx 4rpx rgba(0,0,0,0.1); */
  position: relative;
  width: 336rpx;
  height: 432rpx;
  margin-left: 18rpx;
  background: #fff;
  margin-top: 18rpx;
  display: inline-block;
}
.instrument>view:first-child{
  width: 336rpx;
  height: 304rpx;
  position: relative;
}
.instrument>view:first-child .zdl{
width: 116rpx;
height: 38rpx;
position: absolute;
top: 20rpx;
right: 0;
}
.instrument>view:nth-child(2){
  font-size: 30rpx;
  color: rgb(51,51,51);
  line-height: 30rpx;
  height: 30rpx;
  margin: 26rpx 0 20rpx 10rpx;


display:-webkit-box;
-webkit-line-clamp:1;
-webkit-box-orient:vertical;
overflow:hidden;

}
.instrument>view:nth-child(3){
  height: 30rpx;
  margin-left: 10rpx;
  padding: 10rpx 0 20rpx;
}
.instrument>view:nth-child(3) image{
  width: 30rpx;
  height: 30rpx;
  float: left;
  margin-right: 8rpx;
}
.instrument>view:nth-child(3) text{
  font-size: 24rpx;
  color: rgb(153,153,153);
  line-height: 30rpx;
  float: left;
}

.upMore{
  color: rgb(153,153,153);
  font-size: 24rpx;
  width: 100%;
  text-align: center;
  /* margin: 30rpx 0; */
  padding: 30px 0;
  background: #fff;
}
.zdl{
  position: absolute;
  top: 20rpx;
  right: 0;
  width: 116rpx;
  height: 38rpx;
    line-height: 38rpx;
  z-index: 3;
}
.zdl image{
  width: 100%;
  height: 100%;
}
.zdl text{
  position: absolute;
  height: 100%;
  text-align: center;
  line-height: 38rpx;
  font-size: 24rpx;
  color: #fff;
  display: block;
  top: 0;
  right: 14rpx;
}


.refuseSc{
  position:absolute;width:100;
  bottom: 0;
  left: 0;
  width: 80rpx;
  height: 80rpx;
}